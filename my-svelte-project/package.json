    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
