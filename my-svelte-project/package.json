{
  "name": "my-svelte-project",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode mock",
    "build": "vite build --mode production",
    "build-beta": "vite build --mode beta",
    "build-mock": "vite build --mode mock",
    "preview": "vite preview --mode mock",
    "start": "npm run dev",
    "start-local": "vite --mode localapis",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "test": "jest src",
    "test-watch": "npm run test -- --watch",
    "pretty": "prettier -w \"./src/**/*.*{ts,tsx,vue,svelte,css,scss,html}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged \"./src/**/*.*{ts,tsx,vue,svelte,css,scss,html}\""
    }
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.21",
    "autoprefixer": "^10.4.2",
    "axios": "^0.25.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.1",
    "prettier-plugin-svelte": "^2.6.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-jester": "^2.1.5",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.12",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "vite": "^2.8.6"
  },
  "dependencies": {
    "@builtwithjavascript/formatters": "^1.0.5",
    "i18next": "^21.6.11",
    "svelte-i18n": "^3.3.13"
  }
}
