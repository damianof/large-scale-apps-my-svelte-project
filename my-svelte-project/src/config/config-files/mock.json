{
  "global": {
  },

  "httpClient": {
    "tokenKey": "myapp-token"
  },

  "apiClient": {
    "type": "mock"
  },

  "items": {
    "apiUrls": {
      "fetchItems": "/static/mock-data/items/items.json"
    }
  },

  "localization": {
    "apiUrls": {
      "fetchTranslation": "/static/mock-data/localization/[namespace]/[key].json"
    },
    "locales": [
      { "key": "en-US", "isDefault": true },
      { "key": "it-IT", "isDefault": false }
    ]
  }
}
